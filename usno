default partial alphanumeric_keys
xkb_symbols "basic" {

    include "us(basic)"

    name[Group1]= "USNO";

    key <AD11> {        [ bracketleft,   braceleft,  aring,    Aring    ]        };
    key <AC10> {        [ semicolon,     colon,      oslash,   Ooblique ]        };
    key <AC11> {        [ apostrophe,    quotedbl,   ae,       AE       ]        };

    include "level3(ralt_switch)"
};
